{
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "breadcrumbs.enabled": false,
  "cSpell.language": "en,es",
  "cSpell.userWords": [],
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.fontWeight": "normal",
  "editor.glyphMargin": false,
  "editor.inlineSuggest.enabled": true,
  "editor.matchBrackets": "never",
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on",
    "other": "on"
  },
  "editor.suggestOnTriggerCharacters": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.scrollbar.vertical": "hidden",
  "editor.suggestSelection": "first",
  "editor.wordWrap": "on",
  "files.trimTrailingWhitespace": true,
  "git.confirmSync": false,
  "git.ignoreRebaseWarning": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
  "typescript.validate.enable": true,
  "window.titleBarStyle": "custom",
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "terminal.external.osxExec": "Warp.app",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.startupEditor": "none",
  "workbench.welcomePage.walkthroughs.openOnInstall": false,
  "workbench.tips.enabled": false,
  "workbench.editor.empty.hint": "hidden",
  "editor.autoClosingComments": "always",
  "editor.autoClosingBrackets": "always",
  "editor.formatOnSave": true,
  "files.autoSaveWhenNoErrors": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": "active",
  "editor.linkedEditing": true,
  "terminal.integrated.rightClickBehavior": "copyPaste",
  "terminal.integrated.tabs.focusMode": "singleClick",
  "git.branchProtection": ["production", "prod", "master", "main"],
  "git.defaultBranchName": "dev",
  "git.pullBeforeCheckout": true,
  "javascript.preferences.useAliasesForRenames": false,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "git.enableSmartCommit": true,
  "security.promptForLocalFileProtocolHandling": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.editor.showTabs": "none",
  "editor.folding": false,
  "workbench.editor.enablePreview": false,
  "workbench.editor.closeEmptyGroups": true,
  "zenMode.hideLineNumbers": false,
  "zenMode.centerLayout": true,
  "editor.minimap.enabled": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.guides.indentation": false,
  "editor.lineNumbers": "on",
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "editor.overviewRulerBorder": false,
  "editor.hideCursorInOverviewRuler": true,
  "window.title": "${activeEditorShort}${separator}${rootName}",
  "workbench.colorCustomizations": {
    "editorOverviewRuler.findMatchForeground": "#0000",
    "editorGroup.border": "#00000000",
    "editorGroupHeader.border": "#00000000",
    "sideBar.border": "#2a2a37",
    "panel.border": "#00000000",
    "activityBar.border": "#2a2a37",
    "editorGutter.modifiedBackground": "#6c708040",
    "editorGutter.addedBackground": "#a6e3a140",
    "editorGutter.deletedBackground": "#f3868840",
    "welcomePage.background": "#1e1e2e"
  },
  "editor.scrollbar.verticalScrollbarSize": 0,
  "terminal.integrated.allowChords": false,
  "terminal.integrated.cursorStyleInactive": "underline",
  "terminal.integrated.tabs.location": "left",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "underline",
  "editor.showFoldingControls": "never",
  "typescript.referencesCodeLens.enabled": true,
  "editor.codeLens": true,
  "workbench.editor.editorActionsLocation": "hidden",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.telemetryLevel": "off",
  "workbench.reduceMotion": "on",
  "eslint.run": "onSave",
  "git.autofetch": false,
  "terminal.integrated.tabs.enableAnimation": false,
  "terminal.integrated.suggest.runOnEnter": "always",
  "github.copilot.chat.generateTests.codeLens": true,
  "inlineChat.lineEmptyHint": true,
  "github.copilot.chat.editor.temporalContext.enabled": true,
  "files.simpleDialog.enable": true,
  "editor.cursorBlinking": "smooth",
  "window.autoDetectColorScheme": true,
  "window.autoDetectHighContrast": false,
  "explorer.compactFolders": false,
  "zenMode.fullScreen": false,
  "zenMode.showTabs": "none",
  "zenMode.restore": true,
  "zenMode.hideStatusBar": false,
  "workbench.list.smoothScrolling": false,
  "editor.smoothScrolling": false,
  "editor.cursorSmoothCaretAnimation": "off",
  "terminal.integrated.smoothScrolling": false,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.hg/store/**": true,
    "**/.next/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/.turbo/**": true,
    "**/.cache/**": true,
    "**/coverage/**": true,
    "**/.pnpm-store/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.next": true,
    "**/dist": true,
    "**/build": true,
    "**/.turbo": true,
    "**/.cache": true,
    "**/coverage": true,
    "**/*.min.js": true,
    "**/*.map": true,
    "**/.pnpm-store": true
  },
  "git.autorefresh": false,
  "git.decorations.enabled": false,
  "editor.semanticHighlighting.enabled": false,
  "typescript.tsserver.maxTsServerMemory": 3072,
  "extensions.autoUpdate": false,
  "extensions.autoCheckUpdates": false,
  "window.restoreWindows": "none",
  "editor.renderWhitespace": "none",
  "editor.renderControlCharacters": false,
  "editor.renderLineHighlight": "none",
  "files.autoSave": "off",
  "files.hotExit": "off",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.acceptSuggestionOnCommitCharacter": true,
  "editor.quickSuggestionsDelay": 10,
  "editor.wordBasedSuggestions": "matchingDocuments",
  "github.copilot.editor.enableAutoCompletions": true,
  "editor.suggest.preview": true,
  "editor.suggest.showInlineDetails": true,
  "editor.tabCompletion": "on",
  "editor.parameterHints.enabled": true,
  "workbench.activityBar.location": "top",
  "git.blame.statusBarItem.enabled": false,
  "workbench.preferredDarkColorTheme": "Kanagawa Dragon",
  "window.customTitleBarVisibility": "windowed",
  "editor.stickyScroll.enabled": false,
  "workbench.navigationControl.enabled": false,
  "window.commandCenter": false,
  "window.zoomLevel": 0,
  "editor.mouseWheelZoom": true,
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
  "workbench.iconTheme": "material-icon-theme",
  "claudeCode.selectedModel": "default",
  "workbench.tree.enableStickyScroll": false,
  "terminal.integrated.stickyScroll.enabled": false,
  "workbench.statusBar.visible": false,
  "github.copilot.nextEditSuggestions.enabled": true,
  "chat.viewSessions.orientation": "stacked",
  "chat.commandCenter.enabled": false,
  "workbench.layoutControl.enabled": false
}
